<MusicalWorkNotificationMessage>
    <MessageHeader> <!-- Compulsory -->
        <MessageID>789012</MessageID>
        <MessageCreatedDateTime>2024-06-29T10:00:00Z</MessageCreatedDateTime>
        <Sender>
            <PartyName>MusicLabel</PartyName>
            <PartyID>ML123</PartyID>
        </Sender>
        <Recipient>
            <PartyName>PA-DPIDA-2024072201-A</PartyName> 
            <PartyID>Original Works</PartyID>
        </Recipient>
    </MessageHeader>

    <!-- MusicalWork contains 
    MusicalWorkID: Includes the ISWC of the musical work, linking the musical work to the sound recording.
    Title: The title of the musical work.
    Additional Titles: If the musical work has alternate titles or translations.
    Composer: Information about the composer of the musical work.
    WorkType: To specify the type of the musical work (e.g., Song, Symphony).
    RightShare: Information about the rights associated with the musical work. Explained below
    LinkedResources: his section links the musical work (identified by its ISWC) to the sound recording (identified by its ISRC). This linkage ensures that the registry can associate the musical work with the correct sound recording
    -->
    <MusicalWork>
        <MusicalWorkID> <!-- Compulsory -->
            <ISWC>T-123456789-0</ISWC>
        </MusicalWorkID>
        <Title>My New Song</Title> <!-- Optional -->
        <AlternateTitles> <!-- Optional -->
            <Title>My New Song (Acoustic Version)</Title>
        </AlternateTitles>
        <Composer> <!-- Optional -->
            <ComposerName>Composer Name</ComposerName>
        </Composer>
        <WorkType>Song</WorkType> <!-- Optional -->
       <!-- RightShare
        RightShare: This element encapsulates information about the rights associated with the musical work or sound recording.
        RightShareProprietaryId: A unique identifier for the right share. Usually generated by the party sending the message to uniquely identify each right share within their system.
        RightController: Information about the entity that controls the rights.
        RightType: The types of rights being covered, such as MechanicalRight, PerformingRight, etc. Mulltiple RightType elements can be included.
        -->
        <RightShare>
            <RightShareProprietaryId>12345</RightShareProprietaryId> <!-- Compulsory -->
            <RightController> <!-- Compulsory -->
                <PartyID>PR789012</PartyID>
                <PartyName>MusicPublisher</PartyName>
                <RightControllerName>MusicPublisher</RightControllerName>
            </RightController>
            <RightType>MechanicalRight</RightType> <!-- Compulsory -->
            <RightType>PerformingRight</RightType>
            <Territory>Worldwide</Territory> <!-- Compulsory -->
            <SharePercentage>100</SharePercentage> <!-- Compulsory -->
        </RightShare>

        <LinkedResources> <!-- Optional -->
            <LinkedResource>
                <ResourceType>SoundRecording</ResourceType>
                <ResourceID>
                    <ISRC>US-XYZ-21-12345</ISRC>
                </ResourceID>
            </LinkedResource>
        </LinkedResources>

    </MusicalWork>
</MusicalWorkNotificationMessage>